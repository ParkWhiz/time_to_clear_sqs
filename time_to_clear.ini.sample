[general]
# Name of the SQS queue to track
queue=Queue-Name

# Metrics will be published in the cloudwatch namespace: [namespace]/SQS
namespace=MyCloudwatchNamespace

# AWS credentials. The user needs to have the following permissions:
# cloudwatch:getMetricStatistics, cloudwatch.PutMetricData
aws_access_key_id=MY_AWS_ACCESS_KEY
aws_secret_access_key=MY_AWS_SECRET_ACCESS_KEY
region=us-west-2
